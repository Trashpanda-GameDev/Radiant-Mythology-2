    <Entry>
      <PointerOffset/>
      <JapaneseText>ルーティ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>スタン</JapaneseText>
      <EnglishText/>
