      <PointerOffset/>
      <JapaneseText>やっと、戻ってきたんだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>41</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おかえり、○○。
