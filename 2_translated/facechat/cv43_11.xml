<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>カイウス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ルカ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>イリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ルカとイリアって、仲いいよな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そ、そんな事ないよ。
僕いつも怒られてばっかりだもん…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>カイウスとルビアみたいに
同じ立場で文句言えないし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>同じじゃないから、
上手くいってんじゃないのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…上手くいってるのかな。
でも、もっと、その…、色々意見を
言いたい事もある…かも…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ああ、もう！
何言いたいのかわかんないよ！
言いたい事はズバッと言えばいいんだって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おーい、イリア！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なに〜？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>（ほら、ルカ…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>え、ええっと…。
その…、さ、最近、どう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>は？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なななんでもないよ、
ははは…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>47</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>どーしたの、ルカったら</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>48</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…さあな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>45</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
