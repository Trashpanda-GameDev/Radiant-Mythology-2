  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>○○さん、
リフィルさん達が話していた
「ゲーデ」という者…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>心当たり、ありませんか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>世界樹の傷から、負と共に
現れたあの男です…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…！！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>130</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>同一人物かはわかりませんが、
リフィルさんに伝えた方が
いいんじゃないかと思って</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
