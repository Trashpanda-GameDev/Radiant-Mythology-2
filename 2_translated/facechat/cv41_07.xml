<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ジーニアス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>プレセア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ナナリー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>今日は風が強いね。
あ、あのプレセアっ！
揺れるから足元気をつけてね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>よ、よ、よかったら、
ボクの手を握ってもいいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>不要です。
ジーニアスさんこそ気をつけて下さい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>大丈夫だよ、ボクなら…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ウワッと！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>大丈夫かい？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>な、なんでナナリーがいるんだよぉ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>今日は風で船が揺れるからね。
こうやって様子見て回ってるのさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>子ども扱いしないでよね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>１２歳は、
社会上子どもと判断されます</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>だろ？
特にあんたらは念入りに
見ておくように言われてんのさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…誰に</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ゼロスに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>21</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>アイツ〜…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
