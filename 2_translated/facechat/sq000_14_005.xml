<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ウッドロウ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>今日は、ありがとう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>少しは…、気が楽になった。
いや、まだやらなくてはならない事は
山積みなのだが…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>礼も、先ほど財産は
ほとんど使ってしまったから
大した事は出来ないが…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>私からの心づくしだ。
受け取ってくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
