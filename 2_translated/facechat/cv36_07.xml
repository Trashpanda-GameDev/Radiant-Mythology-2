<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>マオ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>チャット</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>プレセア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>この船って、ホント子どもばっかだネ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そういうあなたが子どもではありませんか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…みなさん。
仲良くしましょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>急にどーしちゃったの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あなたが仕切るなんて、意外ですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>お二人は子どもですから…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>だったらボクに仕切らせてヨ！
なんてったって１３歳なんだからネ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>いえ、でしたら私が…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>何を言うんです。
ボクは船長ですよ？
船長の言う事を優先して下さい！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい…、船長…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あれ？　なんだか
プレセアが落ち込んでるような…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
