<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>クレス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ミント</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>もっと、僕達に出来る事って
あるかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>今回のように薬だけじゃなく、
他にも出来る事はありますよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>薬以外には、やっぱり食料とかかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>毛布なんかもいいかもしれませんね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ハハ、まだ出来る事はありそうだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ミント、やる事は増えるけど
手伝ってくれるかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい、もちろんです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>○○も、
その時はまた
手伝ってもらってもいいかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>オッケー！</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>うーん…</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ありがとう。
君がいると心強いよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>無理は言わないよ。
だって、○○は○○で
いつもがんばっているからさ</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>2</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
