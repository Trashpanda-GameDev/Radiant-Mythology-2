<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>アーチェ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>チェスター</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>○○、
付き合ってくれてありがと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>実はさ、前に採ったギンモクセイの花と
今回のベチバーの根で
ポプリを作ろうと思ってさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>んで、チェスターもやんないと
意味がないの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これは、チェスターの妹に
プレゼントするものだから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あ、チェスターには内緒。
だって、びっくりさせたいもん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おいおい、俺だけに探させといて
なに立ち話してんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ゴメンゴメン。
すぐ行くわよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
