      <JapaneseText>村を出て良かったね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>26</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そうかぁ？</JapaneseText>
