  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>君とは、初対面…だな。
俺はセネル・クーリッジ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>私はクロエ・ヴァレンス。
誇り高き、ヴァレンス家の名を継ぐ者だ。
よろしくお見知りおきいただきたい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>貴族？</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>二人とも知り合い？</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>いや、貴族ではない…。
とにかく、よろしく頼む</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…まあ、仲良くしてやってくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ああ、まあ、そういう事だ</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>37</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なんだ、その迷惑そうな
いい振りは！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
