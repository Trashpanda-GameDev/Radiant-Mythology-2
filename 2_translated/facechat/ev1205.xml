<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>カノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ねえ。
船の改修が終わったんだって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これで、あの海域へ行けるんだね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>恐いけれど、やっぱり私、行きたい。
どうしてだかわかんないけど…、
強く惹かれるの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ニアタ・モナドという名も、
あのカケラにも…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>自分自身にけりをつけなきゃ
いけないし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>本当は、声ばかりを頼って生きていく
わけにはいかないと思ってるの。
変わりたいんだ…、私</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>だから、決めたわ。
私も行くよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
