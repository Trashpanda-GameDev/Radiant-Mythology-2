  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>めずらしい植物でも
見つかればいいけどなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>24</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>うん、初めて見るようなのが
いいよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>新種でも見つけてリフィル先生に
教えたら、ほめてくれるかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>24</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>うん。
だからしっかり調べなくっちゃね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>25</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そか、結局調べ物は
しなきゃなんないのか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>24</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
