<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>リオン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>誰からの依頼だったかって？
その…、民間人からの依頼だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なんだ、疑ってるのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>パニール………、からの又聞きだ。
僕だって詳しい事は知らない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>モンブラン…だっけ？</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>動物たちの依頼でしょ？</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そ、それはパニールが
勝手に申し出ただけだ。
僕から要求したわけじゃない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そんな目で見るな。
…わかった。
一個だけならくれてやる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>二個は駄目だ。
欲しかったら、じかにパニールに頼むんだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…だ、だからどうした。
動物も、民間人も困っているのなら
一緒だろう</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>10</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>依頼人には…、違いない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………行くぞ。
こんな所で時間をつぶしてる
場合じゃないんだからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
