<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ウッドロウ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>フィリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ジェイド</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ゼロス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>く…、流石だな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はぁ…はぁ…。
上手くいかないもの…ですね。
これも神の試練でしょう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>随分と、はた迷惑な神様ですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>まあまあ、ジェイド。
そう言ってやるなって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>んで、船には戻ってくれるんだろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>もちろんだ。
一度、こう羽目を外してみたかった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ウッドロウ。
あんたも損な役回りだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そのウッドロウさんでなければ、
今回の主役は張れなかったと
思います</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これでウッドロウさんの希薄な印象が
払拭出来ればいいんですが</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>それが叶うといいんだがな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>（どうだろうなぁ〜…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>（まあ、過度な期待はよしましょう）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あら、どうかされましたか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>い、いや、別に〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>んじゃ、パッパと戻っちまおうぜぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
