    <Entry>
      <PointerOffset/>
      <JapaneseText>カノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>アニー</JapaneseText>
      <EnglishText/>
