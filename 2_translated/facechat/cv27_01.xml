  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>初めまして、皆さん。
私はミント・アドネードです。
よろしくお願いします</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これはこれは、
大変おしとやかな感じですね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ティアとはまた違うタイプの
大人の雰囲気と申しましょうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ああ、でもティアちゃんと
同じぐらいのボインちゃんじゃん￥
うひょ〜、いい眺めだぜぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…そんな目でしか、
女性を判断出来ないのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ま〜たまた〜。
セネルだって、クロエちゃんを
こっそり見てるくせに〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>昔、抱きついた事があるんだろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おや、それは穏やかではありませんね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あれは事故だ！
故意じゃないぞ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そういやぁ許されるとか
思ってんのかねぇ〜？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>セネルがミントちゃんに抱きついて
しらばっくれるかもしれないから、
俺さま先に抱きついておこうかなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あ、あの、結構です……！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はっ、フラれたな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>無残ですねぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>42</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>とほほ〜…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
