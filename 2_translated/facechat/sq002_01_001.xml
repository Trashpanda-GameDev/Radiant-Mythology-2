  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おまえ自身が忘れていた力、
ずいぶん思い出してきたようだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>だが、もっと引き出せるはずだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おまえが望むのなら、
その手助けをしてもいい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>それが私の役目でもあるからな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
