<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ファラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ルビア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>リッド</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ねえ、ルビア。
リッド見なかった？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>えっと…、ごめん。
あたし、わかんない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あ？　なんか用か？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>15</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>どこ行ってたのよ？
探したんだから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>悪ぃ、ボンヤリ空眺めてたら、
寝ちまってたんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>15</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>もう、しょうがないな〜。
今オムレツ作ったんだけど、
食べる？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>お、もちろん！
へへ、行こーぜぇ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>15</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>羨ましがる</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>妬む</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そうね…。
仲良さそう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>○○も
オムレツが食べたいの？</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>46</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なーんてね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>なんであたし、カイウスと
ケンカばっかりしてんのかな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ファラみたいに何か作って
あげたりすれば…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>って、あたし、
カイウスの好物すら知らないわ…！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>もっと、基本的な事から始めないとね…。
は〜あ…、まずペンじゃなく
包丁で料理出来るようにならなきゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>46</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
