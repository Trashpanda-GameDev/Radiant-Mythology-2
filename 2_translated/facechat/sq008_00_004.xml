<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>バルバトス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>クラトス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…レディアントにディセンダー。
「伝説」だと…？
下らん…、下らんなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>23</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…その「伝説」に、
おまえはなれなかったのだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そして、この時においても…、
おまえは勝てなかった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>抜かせ…っ。
次は、次こそはッ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>23</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>次…か。
…まだ試練は続くようだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>それもレディアントをまとった者の
背負う宿命か…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>28</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
