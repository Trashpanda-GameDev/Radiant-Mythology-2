  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ご苦労様。
どう、彼は元気そうだった？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>130</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…そう、よかった。
安心したわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>えっ…、手紙を預かってる？
どれどれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>まっ、あの人ったら…￥
言わなくてもわかってるのに！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>130</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あら、あらあらあら。
ごめんなさいね。
あなたには、お礼をしなくっちゃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ありがとう、○○さん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
