<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>リフィル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>セネル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ご苦労様。
これで、二つ目はクリアね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…なあ、これって正しい事を
してるのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これで、本当に
世界樹が回復するのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あれこれ考えるばかりじゃ、
何も出来ないわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>言ったでしょう。
やれる事をやりましょうと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>他に世界樹の回復方法など
わからないんだし。
それに…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>私は、ニアタを信じるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ジャニスもリフィルも
あんまり変わらないな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>37</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…そうね。
それは認めるわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
