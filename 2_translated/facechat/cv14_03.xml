<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ファラ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>チャット</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>スタン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あれ…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ねえ、チャットー！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はいはい、
どうしました、ファラさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>このデッキブラシ、
なんでこんな所にしまわれてたの？
すっごいボロっちいけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>まあ、所詮掃除道具ですから。
気にしなくていいんじゃないですか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>お？　デッキブラシじゃん！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そうだけど…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>貸して！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>どうぞ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>おお〜、これは！
手に吸い付くようだ！
頑丈そうだし、使いやすい！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…これは、この船にあったのか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい、
ここにしまわれていたようです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>後でここに返して
おけばいいよね？
じゃ、もうちょっと借りておくよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>何に使うつもりなんでしょう？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そりゃ、ブラシだもん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>どこか掃除するんじゃないの？
まさか剣の稽古に使ったりは
しないと思うけど…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>魔神剣ッ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…使ってるし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>意外な使い方ね…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>16</SpeakerId>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
