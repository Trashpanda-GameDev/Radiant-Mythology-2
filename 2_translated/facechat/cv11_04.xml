<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>チャット</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ティア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>プレセア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ユージーン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ちょ、ちょっとなんですか！
こんな所に無理矢理連れて来て</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>船長。
私はあなたに理解して欲しくて…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>いったい何をです？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ふさふさふわふわの
素晴らしさを…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>嫌ですってば！
動物は嫌いなんです！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あの毛が与える機械への悪影響とか、
糞の臭いとか…。
とにかくボクはお断りです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>大丈夫、徐々になれてもらうから…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>船長、目を閉じておいてね。
プレセア、お願い</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ヒッ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>（あれ？　思ったより気持ち悪くない…。
　毛足が短いから、大丈夫なのかな。
　高級なじゅうたんみたいだ…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>（匂いも全然不快じゃないし…、
　でもこの匂い、嗅いだ事があるような…）</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>って、ユージーンじゃないですか！
あなたまでボクをからかうんですね！
もう知りません！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>18</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…俺はここに連れて来られただけなんだが。
ティア、プレセア…、
説明してくれ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>毛皮…、
ふかふか〜…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>40</SpeakerId>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>にくきゅう…、ぷにぷにぷにぷに…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>30</SpeakerId>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>全く聞いていないな…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
