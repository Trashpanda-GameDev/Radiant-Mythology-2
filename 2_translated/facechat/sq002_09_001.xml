  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ガレットの気候は慣れっこだけど、
他の所は苦手だヨ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>しかし、今の仕事では
そうもいかん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>マオは、あらゆる環境に
対応出来るようにならんとな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>えぇ〜、ひょっとして</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>35</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>外での実地訓練が必要なようだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>34</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
