<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>スタン</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>フィリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <PointerOffset/>
      <JapaneseText>渡す</JapaneseText>
      <EnglishText/>
      <Notes>Choice 1</Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>保留にする</JapaneseText>
      <EnglishText/>
      <Notes>Choice 2</Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ルチルクォーツをくれるって？</JapaneseText>
      <EnglishText/>
      <Notes>Previous Choice 2 leads here</Notes>
      <SpeakerId>8</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>そうか。
ありがとう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ほら、前にフィリアの分も
採りに行かなきゃって
言ってただろ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>だから、また採掘しに行かなきゃって
思ってたんだけど</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>あっ…、フィリアだ。
ちょうどよかった。
おーい、フィリア</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>はい、スタンさん。
何でしょうか</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これ、フィリアの分。
約束してたから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>これは、ルチルクォーツ…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>ありがとうございます。
一生大事にします</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>えっ？
やっぱ、標本が欠けるとそんなに
困るものなのかな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>いえ、でも、これは…。
私にとっては特別なものですから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>11</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>…？？？
そうか。
まあ、でも喜んでくれてよかったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset/>
      <JapaneseText>○○、ありがとう。
おかげで、フィリアとの約束が
守れたよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
